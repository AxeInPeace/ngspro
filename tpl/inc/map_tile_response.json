{{ callback }}({
    "type": "FeatureCollection",
    "features": [
    {
        {% for balloon in data %}
        "type": "Feature", 
        "id": {{ balloon.id }},
        "geometry": {
            "coordinates": [{{ balloon.lat }}, {{ balloon.lng }}],
            "type": "Point"
        },
        "properties": {
                "balloonContentHeader": "<h5 class=\"info__title-center\">{{ balloon.title }}</h5><h5 class=\"map__custom-control__text text-grey\">Широта: {{ balloon.lat }} </h5><h5 class=\"map__custom-control__text text-grey\">Долгота: {{ balloon.lng }} </h5><h5 class=\"map__custom-control__text text-grey\">Альтитуда: {{ balloon.height }} <h5><hr/>",
            "balloonContentBody": "<h5>Тип пункта: {{ balloon.get_type_display }} </h5><h5>Класс точности: {{ balloon.precision }} </h5><h5>Ориентирный знак: {% if baloon.backsight %}сохранился{% else %}не сохранился{% endif %}</h5><h5>Наружный знак: {% if baloon.outer %}сохранился{% else %}не сохранился{% endif %}</h5><h5>Центр: {% if baloon.center %}сохранился{% else %}не сохранился{% endif %}</h5><h5>Положение центра относительно уровня земли: {{ balloon.center_height }}, м</h5>{% if balloon.images %}<div class=\"images\"><span class=\"glyphicon glyphicon-chevron-left\"></span>{% for img in balloon.images %}<img src=\"/pic/preview.jpg\" class=\"info__img-prev\"/><a href=\"{{ trgstation.center_photo.url }}\" class=\"fancybox info__img-prev\" title=\"Фото центра\"><img src=\"{{ img.url }}\" width=\"128\" height=\"128\"/></a>{% endfor %}<span class=\"glyphicon glyphicon-chevron-right\"></span>{% endif %}</div>",
            "balloonContentFooter": "<div class=\"info__btns\"><button type=\"button\" class=\"btn btn-primary info__btn-download\">Скачать</button><button type=\"button\" class=\"btn btn-danger info__btn-close\">Закрыть</button></div>",
            "clusterCaption": "Метка 2"
        }, 
        "options": {
            "iconLayout": 'default#image',
            "iconImageHref": 'https://upload.wikimedia.org/wikipedia/commons/thumb/1/12/Japanese_Map_symbol_(Triangulation_point).svg/2000px-Japanese_Map_symbol_(Triangulation_point).svg.png',
            "iconImageSize": [32, 32],
            "iconImageOffset": [-16, -16]
        }
    {% endfor %}
}]});
